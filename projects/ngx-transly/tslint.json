{
  "extends": "../../tslint.json",
  "rules": {
    "curly": false,
    "directive-selector": [
      true,
      "attribute",
      "lib",
      "camelCase"
    ],
    "component-selector": [
      true,
      "element",
      "lib",
      "kebab-case"
    ],
    "no-string-literal": false,
    "typedef": false
  }
}
